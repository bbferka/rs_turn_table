  <analysisEngineMetaData>
    <name>ttable</name>
    <description>Simple pipeline for capturing object from the turn table</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="none">

      <configurationParameter>
        <name>camera_config_files</name>
        <description>path to config file that holds info about camera topics and tf frame names</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>CollectionReader/camera_config_files</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
          <name>minX</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
          <overrides>
            <parameter>PointCloudFilter/minX</parameter>
          </overrides>
        </configurationParameter>

        <configurationParameter>
          <name>maxX</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
          <overrides>
            <parameter>PointCloudFilter/maxX</parameter>
          </overrides>
        </configurationParameter>

        <configurationParameter>
          <name>minY</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
          <overrides>
            <parameter>PointCloudFilter/minY</parameter>
          </overrides>
        </configurationParameter>

        <configurationParameter>
          <name>maxY</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
          <overrides>
            <parameter>PointCloudFilter/maxY</parameter>
          </overrides>
        </configurationParameter>

        <configurationParameter>
          <name>minZ</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
          <overrides>
            <parameter>PointCloudFilter/minZ</parameter>
          </overrides>
        </configurationParameter>

        <configurationParameter>
          <name>maxZ</name>
          <type>Float</type>
          <multiValued>false</multiValued>
          <mandatory>true</mandatory>
          <overrides>
            <parameter>PointCloudFilter/maxZ</parameter>
          </overrides>
        </configurationParameter>


      <configurationParameter>
        <name>plane_estimation_mode</name>
        <description>BOARD or PCL or MPS or from FILE</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
            <parameter>PlaneAnnotator/plane_estimation_mode</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>segmMode</name>
        <description>switch between Inverse binary and binary thresholding</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
            <parameter>ImageSegmentationAnnotator/segmMode</parameter>
        </overrides>
      </configurationParameter>

    </configurationParameters>

    <configurationParameterSettings>
      <nameValuePair>
        <name>camera_config_files</name>
        <value>
          <array>
            <string>config_xtion.ini</string>
          </array>
        </value>
      </nameValuePair>

        <nameValuePair>
          <name>maxZ</name>
          <value>
            <float>1.35</float>
          </value>
        </nameValuePair>
        <nameValuePair>
          <name>minZ</name>
          <value>
            <float>0</float>
          </value>
        </nameValuePair>
        <nameValuePair>
          <name>maxX</name>
          <value>
            <float>0.2</float>
          </value>
        </nameValuePair>
        <nameValuePair>
          <name>minX</name>
          <value>
            <float>-0.4</float>
          </value>
        </nameValuePair>
        <nameValuePair>
          <name>maxY</name>
          <value>
            <float>0.4</float>
          </value>
        </nameValuePair>
        <nameValuePair>
          <name>minY</name>
          <value>
            <float>-0.2</float>
          </value>
        </nameValuePair>

        <nameValuePair>
          <name>plane_estimation_mode</name>
          <value>
              <string>FILE</string>
          </value>
        </nameValuePair>

      <nameValuePair>
        <name>segmMode</name>
        <value>
          <string>INVBINARY</string>
        </value>
      </nameValuePair>

    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
      <node>MoveTurnTable</node>
      <node>CollectionReader</node>
      <node>ImagePreprocessor</node>
      <node>PointCloudFilter</node>
      <node>PlaneAnnotator</node>
      <node>PointCloudClusterExtractor</node>
